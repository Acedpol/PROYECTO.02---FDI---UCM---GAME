<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <TargetName>$(ProjectName)</TargetName>
    <IncludePath>$(SolutionDir)LIBS\drakhtar-i18n\include;$(IncludePath)</IncludePath>
    <LibraryPath>$(SolutionDir)LIBS\drakhtar-i18n\lib;$(LibraryPath)</LibraryPath>
    <PublicModuleDirectories>$(ProjectDir)\src;</PublicModuleDirectories>
    <IntDir>$(SolutionDir)Temp\$(Configuration)\</IntDir>
    <OutDir>$(SolutionDir)bin\$(Configuration)\</OutDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link>
      <AdditionalDependencies>i18n.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <SubSystem>Console</SubSystem>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>